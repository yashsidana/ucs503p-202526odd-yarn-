// Code Flowchart
digraph {
	CODE [label="Code Overview" color=lightblue shape=box style=filled]
	i [label="Variable: i" color=red shape=note]
	CODE -> i
	t2 [label="Variable: t2" color=red shape=note]
	CODE -> t2
	nextTerm [label="Variable: nextTerm" color=red shape=note]
	CODE -> nextTerm
	t1 [label="Variable: t1" color=red shape=note]
	CODE -> t1
}
